#!/bin/bash

make distclean

if [ "$1" != "clean" ]
then
	../configure --target=$1 CPPFLAGS=-DDEBUG CXXFLAGS="-g -O0" && printf "\nStarting build with target=$1\n" && make -j 10 > log 2> err
#	../configure --target=$1 CPPFLAGS=-DDEBUG CXXFLAGS="-g -O0" && printf "\nStarting build with target=$1\n" && make -j 10 &> log
fi
